nav {
    border-top: 1px solid #000000;
    border-bottom: 1px solid #000000;
    background-color: #837F7F;
    text-align: left;
    min-height: 200px;
    
}
.first {
    margin-left: 5%;   
}
section {
    align-content: center;   
}
nav li {
    display: inline-block;
    text-align: center;
    font-size: 42px;
    font-weight: bold;
    margin-left: 10px;
    margin-right: 10px;
    margin-top: 40px;
}
a {
    text-decoration: none;   
}
header {
    background-color: #877963;
    text-align: center;
    margin-top: 0px;
    
}
body {
    background-color: #141414;
    min-width: 1200px;
    margin: 0px;
}
#canvas {
    background-color: #96CEB4;
    height: 200px;
    width: 100px;
}
#logo {
 float: left;   
}
#spefs {
    margin-top: 20px;
    margin-left: 34%; 
}
p {
    text-align: center;
    color: #FFF;
}
h1{
    text-align: center;
    color: #FFF;
}
#slideshow {
   margin-left: 34%;
}
#text {
    margin-top: 0px;   
}
footer {
    position:absolute;
    bottom:0;
    width:100%;
    background-color: #877963;
    text-align: center;
}
footer p {
    color: #FFF;   
    font-size: 12px;
}